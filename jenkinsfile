pipeline {
    agent any

    tools {
        jdk 'JDK21' 
    }

    stages {
        stage('Checkout code') {
            steps {
                git branch: 'master', url: 'https://github.com/ahmedkbenzid/TP1-DEVOPS.git'
            }
        }

        stage('Compile code') {
            steps {
                sh 'javac -d . src/Main.java'
            }
        }

        stage('Execute code') {
            steps {
                sh 'java Main'
            }
        }
        stage('display message'){
            steps{
                echo 'Hello from Github'
            }
        }
    }
}
